{
	"name":              "WordPress-Plugins/ufhealth-gravity-forms-secure-storage",
	"description":       "Adds a secure storage layer to Gravity Forms to fascilitate our ability to handle various data requirements.",
	"type":              "wordpress-plugin",
	"keywords":          [],
	"authors":           [
		{
			"name":     "UF Health",
			"email":    "webservices@ahc.ufl.edu",
			"homepage": "http://webservices.ufhealth.org/",
			"role":     "Developer"
		}
	],
	"minimum-stability": "dev",
	"require":           {
		"php": ">=5.6",
		"tozny/e3db": "dev-master"
	},
	"post-install-cmd":  [
		"wp-enforcer"
	],
	"post-update-cmd":   [
		"wp-enforcer"
	],
	"require-dev": {
		"wp-cli/wp-cli": "^1.5@dev",
		"phpunit/phpunit": "6.5.*",
		"stevegrunwell/wp-enforcer": "dev-master"
	}
}
